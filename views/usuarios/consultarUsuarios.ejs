<section>
    <h2>Usuarios Activos - <span id="textCambio">Sin Cambios</span>
    </h2>

    <ul>
        <% activeUsers.forEach(function(user) { %>
        <li>
            <%= user.IDUsuario %> - <%= user.CorreoElectronico %> - <%= user.Status %>
            <button onclick="modifyStatus('<%= user.IDUsuario %>', '<%= user.Status %>')">
                <% if (user.Status == 0) { %>
                    Activar
                <% } else { %>
                    Desactivar
                <% } %>
            </button>
        </li>
        <% }); %>
    </ul>

    <h2>Usuarios Inactivos</h2>
    <ul>
        <% inactiveUsers.forEach(function(user) { %>
        <li>
            <%= user.IDUsuario %> - <%= user.CorreoElectronico %> - <%= user.Status %>
            <button onclick="modifyStatus('<%= user.IDUsuario %>', '<%= user.Status %>')">
                <% if (user.Status == 0) { %>
                    Activar
                <% } else { %>
                    Desactivar
                <% } %>
            </button>
        </li>
        <% }); %>
    </ul>

    <h2>Administradores</h2>
    <ul>
        <% adminUsers.forEach(function(admin) { %>
        <li>
            <%= admin.IDUsuario %> - <%= admin.CorreoElectronico %> - <%= admin.Status %>
            <button onclick="modifyStatus('<%= admin.IDUsuario %>', '<%= admin.Status %>')">
                <% if (admin.Status == 0) { %>
                    Activar
                <% } else { %>
                    Desactivar
                <% } %>
            </button>
        </li>
        <% }); %>
    </ul>
</section>
<script type="text/javascript" src="/js/usuarios/consultarUsuarios.js"></script>